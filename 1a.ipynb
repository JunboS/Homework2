    "import random\n",
    "if __name__ == '__main__':\n",
    "    f = open(\"1a.csv\", \"a\")\n",
    "    numbers = []\n",
    "    for x in range(1000):\n",
    "        numbers.append(random.randint(0, 100))\n",
    "    #sort\n",
    "    numbers.sort()\n",
    "    for num in numbers:\n",
    "        f.write(str(num)+'\\n')\n",
    "\n",
    "    print(numbers)\n",
    "    f.close()\n"
